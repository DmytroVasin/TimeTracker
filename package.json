{
  "name": "TimeTracker",
  "version": "0.0.1",
  "description": "App is based on Electron, React, Redux + Rails as a back end.",
  "main": "app/js/index.js",
  "author": "Dmytro Vasin <dmytro.vasin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/DmytroVasin/XXX.git"
  },
  "scripts": {
    "start": "NODE_ENV=development electron ./app/js/index.js",
    "watch": "webpack-dev-server --hot --inline",
    "start:build": "NODE_ENV=production electron ./app/js/index.js",
    "build": "NODE_ENV=production webpack --display-error-details",
    "postinstall": "install-app-deps",
    "dist": "build -mwl --x64 --ia32",
    "clean:osx": "rm -rf ./app_build",
    "build:osx": "electron-packager . \"TimeTracker\" --out=app_build/osx  --platform=darwin --arch=x64 --icon=app/build/time-tracker.icns --ignore=node_modules/electon-* --ignore=browser-app/src/*.* --ignore=.gitignore --ignore=webpack.config.js"
  },
  "dependencies": {
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^7.7.0",
    "electron-positioner": "^3.0.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "moment": "^2.14.1",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "axios": "^0.13.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.24.0",
    "electron": "^1.3.4",
    "electron-builder": "^6.3.0",
    "electron-prebuilt": "^1.3.4",
    "firebase": "^3.2.0",
    "node-sass": "^3.8.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "build": {
    "appId": "hr.creaticon.eatodo",
    "app-category-type": "public.app-category.productivity",
    "dmg": {},
    "win": {
      "iconUrl": "http://eatodo.s3.amazonaws.com/icon.ico"
    }
  },
  "buildAbout": {
    "appName": "Time Tracker",
    "appRepo": "https://github.com/DmytroVasin/XXX",
    "apiRepo": "https://github.com/DmytroVasin",
    "apiApp": "https://time-tracker-api.herokuapp.com/"
  }
}
